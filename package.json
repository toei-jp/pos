  "scripts": {
    "ng": "ng",
    "start-client": "ng serve --environment=local",
    "build-client": "ng build --configuration=production && ng build --configuration=test && ng build --configuration=development",
    "build-client-local": "ng build --configuration=local",
    "test-client": "ng test --code-coverage",
    "lint-client": "tslint --config tslint.json src/client/**/**.ts --exclude **/**.spec.ts --project src/client/tsconfig.json",
    "e2e": "ng e2e",
    "start": "node dist/server/index",
    "ssl": "node dist/server/ssl",
    "build-server": "tsc --project src/server --outDir dist/server --locale ja-jp",
    "lint-server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
    "check": "npm run lint",
    "lint": "npm run lint-client && npm run lint-server",
    "test": "npm run check",
    "build": "npm run build-server && npm run build-client",
    "preversion": "npm run build",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "clean": "rimraf src/**/*.spec.ts",
    "update": "npm install @cinerino/api-javascript-client@next @cinerino/api-nodejs-client@next"
  },
  "private": true,
